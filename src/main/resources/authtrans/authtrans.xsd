	<xs:complexType name="Status_Type">
		<xs:annotation>
			<xs:documentation>Статус ответа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StatusCode" type="StatusCode_Type">
				<xs:annotation>
					<xs:documentation>Статусный код возврата</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StatusDesc" type="StatusDesc_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание статуса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StatusType" type="StatusType_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Тип ошибки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ServerStatusDesc" type="ServerStatusType_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание статуса системы источника</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalStatus" type="AdditionalStatus_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительный статус. Этот агрегат может содержать дополнительные статусы, которые может сообщить сервер. Например, и
						номер счета, и дата неправильны, то StatusCode может содержать одну из этих ошибок, а этот агрегат – другую.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="StatusCode_Type">
		<xs:annotation>
			<xs:documentation>
				Статусный код возврата.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Long"/>
